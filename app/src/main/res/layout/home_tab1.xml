<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        style="@style/TextSizeBlack16"
        android:layout_marginTop="@dimen/dms_30"
        android:text="@string/everyone_playing" />

    <TextView
        android:layout_width="@dimen/dms_108"
        android:layout_height="@dimen/dms_4"
        android:layout_margin="@dimen/dms_20"
        android:background="@color/orange_fdb931" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dms_18">

        <TextView
            android:id="@+id/home_tab1_1"
            style="@style/style_home_play_item"
            android:text="@string/home_tab_play" />

        <TextView
            android:id="@+id/home_tab1_2"
            style="@style/style_home_play_item"
            android:text="@string/home_tab_service" />

        <TextView
            android:id="@+id/home_tab1_3"
            style="@style/style_home_play_item"
            android:text="@string/home_tab_eat_live" />

        <TextView
            android:id="@+id/home_tab1_4"
            style="@style/style_home_play_item"
            android:text="@string/home_tab_periphery" />

        <!--<TextView-->
            <!--android:id="@+id/home_tab1_5"-->
            <!--style="@style/style_home_play_item"-->
            <!--android:text="@string/home_tab_periphery" />-->
    </LinearLayout>
</LinearLayout>