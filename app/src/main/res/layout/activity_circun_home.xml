<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/circun_cate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dms_46"
        android:text="美食"/>
    <TextView
        android:id="@+id/circun_hotel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dms_46"
        android:text="酒店"/>
    <TextView
        android:id="@+id/circun_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dms_46"
        android:text="购物"/>
    <TextView
        android:id="@+id/circun_traffic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dms_46"
        android:text="交通"/>
    <TextView
        android:id="@+id/circun_scenic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dms_46"
        android:text="景区"/>
</LinearLayout>